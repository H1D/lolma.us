<title>{{model.title}}</title>

{{#if model.redirectToEn}}
  <meta http-equiv="refresh" content="0; url=/en/blog/" />
{{/if}}

{{#if model.linkedData}}
  <script type="application/ld+json">
    {{~json-stringify model.linkedData~}}
  </script>
{{/if}}
